class Solution:
    def wordBreak(self, line, dictionary):
        n = len(line)
        dp = [False] * (n + 1)
        dp[0] = True
        
        for i in range(1, n + 1):
            for word in dictionary:
                if i >= len(word) and dp[i - len(word)] and line[i - len(word):i] == word:
                    dp[i] = True
                    break
        
        return dp[n]